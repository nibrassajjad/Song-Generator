pad_sequences\n\n# Load the lyrics file for the chosen artist\nfile_path = '/kaggle/input/poetry/michael-jackson.txt'  # Load the chosen artist/band file\nwith open(file_path, 'r', encoding='utf-8') as file:\n    text = file.read()\n\n# Preprocessing the text\ntext = text.lower()  # Convert to lowercase\ntext = re.sub(r'\\[.*?\\]', '', text)  # Remove text within square brackets\ntext = re.sub(r'\\s+', ' ', text)  # Replace newlines and multiple spaces with single spaces\ntext = re.sub(r'[^\\w\\s]', '', text)  # Remove punctuation\n\n# Tokenize the text into words\ntokenizer = Tokenizer()\ntokenizer.fit_on_texts([text])\nword_index = tokenizer.word_index\n\n# Convert text to sequences of integers\nsequences = tokenizer.texts_to_sequences([text])[0]\n\nprint(f\"Total words (unique tokens): {len(word_index)}\")\n\n# Display the mapping of the first 20 words in the corpus\nfirst_20_words = text.split()[:20]\nfirst_20_word_indices = [word_index[word] for word in first_20_words]\n\n# Format the output\nformatted_output = ', '.join([f\"{word}: {index}\" for word, index in zip(first_20_words, first_20_word_indices)])\nprint(f\"First 20 words in the corpus: [{formatted_output}]\")\n\n# Verify the type of word_index\nprint(f\"Type of word_index after tokenizer: {type(word_index)}\")\nprint(word_index)\n","metadata":{"execution":{"iopub.status.busy":"2024-08-04T07:10:00.038256Z","iopub.execute_input":"2024-08-04T07:10:00.039568Z","iopub.status.idle":"2024-08-04T07:10:00.198582Z","shell.execute_reply.started":"2024-08-04T07:10:00.039530Z","shell.execute_reply":"2024-08-04T07:10:00.197555Z"},"trusted":true},"execution_count":21,"outputs":[{"name":"stdout","text":"Total words (unique tokens): 3159\nFirst 20 words in the corpus: [you: 1, ever: 168, want: 90, something: 288, that: 15, you: 1, know: 26, you: 1, shouldnt: 956, have: 75, the: 2, more: 187, you: 1, know: 26, you: 1, shouldnt: 956, have: 75, it: 7, the: 2, more: 187]\nType of word_index after tokenizer: <class 'dict'>\